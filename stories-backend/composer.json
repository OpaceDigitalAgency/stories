{
    "name": "stories/api",
    "description": "Stories From The Web API",
    "type": "project",
    "require": {
        "php": ">=7.4",
        "firebase/php-jwt": "^6.0"
    },
    "autoload": {
        "psr-4": {
            "StoriesAPI\\": "api/v1/"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    }
}